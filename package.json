{
  "name": "nest-rest",
  "version": "0.0.1",
  "description": "NestJS Project",
  "author": "Muhammad Yousuf",
  "license": "MIT",
  "keywords": [
    "nestjs",
    "typescript",
    "graphql"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/MuhammadYousufHere/delite.git"
  },
  "bugs": {
    "url": "https://github.com/MuhammadYousufHere/delite/issues"
  },
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "start": "nest start",
    "watch": "webpack --config ./webpack/webpack.watch.js",
    "build:prod": "EXPOSE_SCHEMA=false BUNDLE_MODULES=true webpack --config ./webpack/webpack.build.js",
    "start:dev": "nest start --watch",
    "format": "prettier --write 'schema/**/*.graphql' 'src/**/*.ts' 'test/**/*.ts'",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "test": "jest",
    "prepare": "husky install",
    "migrate:dev": "prisma migrate dev --preview-feature",
    "migrate:dev:create": "prisma migrate dev --create-only --preview-feature",
    "migrate:reset": "prisma migrate reset --preview-feature",
    "migrate:deploy": "npx prisma migrate deploy --preview-feature",
    "migrate:status": "npx prisma migrate status --preview-feature",
    "prisma:generate": "npx prisma generate",
    "prisma:generate:watch": "npx prisma generate --watch",
    "seed": "prisma seed db",
    "docker": "docker-compose up -d",
    "docker:seed": "docker exec -it nest-js-rest yarn run seed",
    "docker:db": "docker-compose -f docker-compose.db.yml up -d",
    "docker:build": "docker-compose build",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@apollo/server": "^4.9.3",
    "@apollo/subgraph": "^2.5.4",
    "@aws-sdk/client-s3": "^3.405.0",
    "@neo4j/graphql": "^4.0.0",
    "@nestjs/apollo": "^12.0.7",
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.1.1",
    "@nestjs/core": "^10.0.0",
    "@nestjs/graphql": "^12.0.8",
    "@nestjs/jwt": "^10.1.1",
    "@nestjs/platform-express": "^10.0.0",
    "@prisma/client": "^5.3.0",
    "bcrypt": "^5.1.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "graphql": "^16.8.0",
    "graphql-middleware": "^6.1.35",
    "graphql-scalars": "^1.22.2",
    "jsonwebtoken": "^9.0.2",
    "neo4j-driver": "^5.12.0",
    "on-headers": "^1.0.2",
    "prom-client": "^14.2.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^5.0.1",
    "rxjs": "^7.8.1",
    "url": "^0.11.1",
    "winston": "^3.10.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@types/express": "^4.17.17",
    "@types/graphql-upload": "^16.0.1",
    "@types/jest": "^29.5.2",
    "@types/node": "^20.3.1",
    "@types/on-headers": "^1.0.0",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "circular-dependency-plugin": "^5.2.2",
    "copy-webpack-plugin": "^11.0.0",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-webpack-plugin": "^4.0.1",
    "fork-ts-checker-webpack-plugin": "^8.0.0",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "lint-staged": "^14.0.1",
    "nodemon": "^3.0.1",
    "prettier": "^3.0.0",
    "prisma": "^5.3.0",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "terser-webpack-plugin": "^5.3.9",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4",
    "webpack-graphql-loader": "^1.0.2",
    "webpack-merge": "^5.9.0",
    "webpack-node-externals": "^3.0.0",
    "webpack-shell-plugin-next": "^2.3.1"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  }
}
